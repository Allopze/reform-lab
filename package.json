{
  "name": "office-to-pdf-converter",
  "version": "1.0.0",
  "description": "Simple Office to PDF converter usando LibreOffice headless",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "watch": "nodemon server.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:up-build": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build"
  },
  "keywords": [
    "office",
    "pdf",
    "converter",
    "libreoffice"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@imgly/background-removal-node": "^1.4.5",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "bmp-js": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "jimp": "^0.22.12",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "png-to-ico": "^2.1.8",
    "potrace": "^2.1.8",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1"
  },
  "overrides": {
    "zod": "3.25.76"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
